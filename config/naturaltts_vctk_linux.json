{
    "run_name": "CanoNaturalSpeech",
    "run_description": "Cano Speech running on VCTK dataset",
    "output_path": "/home/output",

    "dataset_config": {
        "datasets": [
            {
                "dataset_name": "vctk",
                "meta_file_train": "/home/canoSpeech/filelists/vctk_train_filelist.txt.cleaned",
                "meta_file_val": "/home/canoSpeech/filelists/vctk_test_filelist.txt.cleaned",
                "path": "/home/dataset/VCTK",
                "language": "en"
            }
        ],
        "num_loader_workers": 4,
        "num_eval_loader_workers": 4,
        "use_cache": false,
        "add_preprocess_data": true
    },
    "text": {
        "min_text_len": 1,
        "max_text_len": 190,
        "add_blank": true,
        "cleaned_text": true,
        "text_cleaners": {"en": "english_cleaners2"}
    },
    "audio": {
        "mel_fmin": 0,
        "mel_fmax": null,
        "hop_length": 256,
        "win_length": 1024,
        "sample_rate": 22050,
        "fft_size": 1024,
        "num_mels": 80,
        "pitch_fmax": 640.0,
        "pitch_fmin": 1.0,
        "max_audio_length": 10.0,
        "min_audio_length": 1.0
    },

    "eval_split_max_size": 256,
    "eval_split_size": 0.01,
    "distributed_backend": "nccl",
    "distributed_url": "tcp://localhost:54321",
    "mixed_precision": false,
    "precision": "fp16",
    "grad_clip": [0.0, 0.0],
    "use_grad_scaler": false,
    "cudnn_enable": true,
    "cudnn_deterministic": false,
    "cudnn_benchmark": true,

    "scheduler_after_epoch": true,
    "lr": [2e-4, 2e-4],
    "optimizer": "AdamW",
    "optimizer_params": {
        "betas": [0.8, 0.99 ],
        "eps": 1e-09,
        "weight_decay": 0.01
    },
    "lr_scheduler": "ExponentialLR",
    "lr_scheduler_params": {
        "gamma": 0.99875,
        "last_epoch": -1
    },

    "epochs": 1000,
    "batch_size": 32,
    "eval_batch_size": 32,

    "print_step": 30,
    "plot_step": 100,
    "log_model_step": 1000,
    "save_step": 14000,
    "save_n_checkpoints": 10,
    "save_checkpoints": true,

    "loss": {
        "kl_loss_alpha": 1.0,
        "kl_loss_forward_alpha": 1.0,
        "disc_loss_alpha": 100.0,
        "gen_loss_alpha": 100.0,
        "gen_loss_e2e_alpha": 100.0,
        "feat_loss_alpha": 10.0,
        "mel_loss_alpha": 50.0,
        "dur_loss_alpha": 0.1,
        "pitch_loss_alpha": 0.1,
        "speaker_encoder_loss_alpha": 9.0,
        "use_soft_dynamic_time_warping": false
    },

    "model": {
        "hidden_channels": 192,
        "out_channels":513,
        "spec_segment_size": 32,

        "language_embedding_channels": 4,
        "use_language_embedding": false,
        "speaker_embedding_channels": 512,
        "use_speaker_ids": false,
        "use_speaker_encoder_as_loss": false,
        "use_gt_duration": true,

        "text_encoder": {
            "num_chars": 178,
            "hidden_channels_ffn": 768,
            "num_heads": 2,
            "num_layers": 10,
            "kernel_size": 3,
            "dropout_p": 0.1
        },
        "audio_encoder": {
            "kernel_size": 5,
            "dilation_rate": 1,
            "num_layers": 16
        },
        "flow": {
            "kernel_size": 5,
            "dilation_rate": 1,
            "num_flows": 4,
            "num_layers_in_flow": 4,
            "attention_heads": 2
        },
        "duration_predictor": {
            "kernel_size": 3,
            "n_stack": 10,
            "n_stack_in_stack": 3,
            "attention_num_head": 2,
            "dropout_p": 0.5
        },
        "pitch_predictor": {
            "kernel_size": 3,
            "n_stack": 10,
            "n_stack_in_stack": 3,
            "attention_num_head": 2,
            "dropout_p": 0.5
        },
        "learnable_upsampling": {
            "d_predictor": 192,
            "kernel_size_lu": 3,
            "dropout_lu": 0.0,
            "conv_output_size": 8,
            "dim_w": 4,
            "dim_c": 2,
            "max_seq_len": 1000
        },
        "waveform_decoder": {
            "resblock_type": "1",
            "resblock_dilation_sizes": [
                [ 1, 3, 5 ],
                [ 1, 3, 5 ],
                [ 1, 3, 5 ]
            ],
            "resblock_kernel_sizes": [ 3, 7, 11 ],
            "upsample_kernel_sizes": [ 16, 16, 4, 4 ],
            "upsample_initial_channel": 512,
            "upsample_rates": [ 8, 8, 2, 2 ]
        },
        "quantizer": {
            "num_quantizers": 8,
            "codebook_size": 1024,
            "codebook_dimension": 192,
            "codebook_loss_alpha": 1.0
        },
        "discriminator": {
            "periods_multi_period": [2, 3, 5, 7, 11],
            "use_spectral_norm": false
        }
    }
}
